<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MobilePhone</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Mobile_Phone__c , &quot;([\\d]{11})?&quot;))</errorConditionFormula>
    <errorMessage>Numarayı doğru giriniz</errorMessage>
</ValidationRule>
