<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enrolment_date_rule</fullName>
    <active>true</active>
    <errorConditionFormula>YEAR( TODAY() )  &lt;&gt; YEAR( Enrollment_Date__c )</errorConditionFormula>
    <errorDisplayField>Enrollment_Date__c</errorDisplayField>
    <errorMessage>Enrollment date should be current date</errorMessage>
</ValidationRule>
